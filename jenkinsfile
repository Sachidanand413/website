pipeline{
    agent any
    stages{
        stage('Git Checkout'){
            steps{
                git credentialsId: 'Azure-Devops-repo', url: 'almatoolbox@vs-ssh.visualstudio.com:v3/almatoolbox/SachidanandProject/SachidanandProject'
            }
        }
        stage('Maven build'){
            steps{
                sh 'mvn clean install'
            }
        }
    }
}
